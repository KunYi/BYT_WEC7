; TITLE:        BOOT.BIB
;
;
; Windows CE Boot Loader Source Module
;
;******************************************************************************
MEMORY
;   Name     Start     Size      Type
;   -------  --------  --------  ----
    EBOOT    00130000  00050000  RAMIMAGE
    RAM      00180000  0007F000  RAM
    BOOTARGS 001FF000  00001000  RESERVED   ; Boot arguments, see also BOOT_ARG_PTR_LOCATION, BOOT_ARG_LOCATION_NP, BOOT_ARG_PTR_LOCATION_NP
    ETHDMA   00200000  00020000  RESERVED   ; Eboot Ethernet, see ETHDMA_BUFFER_BASE

; Total reserved area, which equals offset of the NK region in RAM
    dwReservedArea    00000000    00220000    FIXUPVAR
 
CONFIG
    COMPRESSION=OFF
    KERNELFIXUPS=ON

MODULES
;   Name            Path                                            Memory Type
;   --------------  ----------------------------------------------  -----------
    nk.exe          $(SG_OUTPUT_ROOT)\platform\$(_TGTPLAT)\target\$(_TGTCPU)\$(WINCEDEBUG)\eboot.exe EBOOT
    
