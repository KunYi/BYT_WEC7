	$(_PLATLIB)\$(_CPUDEPPATH)\SDHCBase.lib \
	$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\SDHClib.lib \
	$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\SDCardLibBase.lib
  
