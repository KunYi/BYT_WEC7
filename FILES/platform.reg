    "DistanceTolerance"=dword:32   ; 0.5 thousandths of an inch
    "AngularTolerance"=dword:871C  ; PI/9.0 (20 degrees)
    "ExtraInfo"=dword:871C         ; PI/9.0 (20 degrees)
