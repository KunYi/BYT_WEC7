;== Common part, shared between different _TGTPROJ =============================

;-- Shared KITL IRQ ------------------------------------------------------------
; This key controls disables IRQ sharing with the KITL NIC.
; IRQ sharing with the KITL NIC can cause debugger hangs if
; there is a small flaw in any driver or hardware on that IRQ.
; @XIPREGION IF PACKAGE_OEMXIPKERNEL
; HIVE BOOT SECTION
IF BSP_ALLOWKITLSHAREDIRQ !
[HKEY_LOCAL_MACHINE\Platform]
    "DisableKITLSharedIRQ"=dword:1
ENDIF BSP_ALLOWKITLSHAREDIRQ
; END HIVE BOOT SECTION
; @XIPREGION ENDIF PACKAGE_OEMXIPKERNEL
;-------------------------------------------------------------------------------

;-------------------------------------------------------------------------------
; @CESYSGEN IF CE_MODULES_DEVICE
;;;; Entering this system power state shuts down the system.  All devices are powered off,
;;;; resuming may require user intervention.  The system will cold boot on resume.
;;;; Supporting this power state requires that the OEM customize the Power Manager
;;;; to recognize POWER_STATE_OFF and take platform-specific action to remove
;;;; power.
;;;;
[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Power\State\ShutDown]
    "Default"=dword:4           ; D4
    "Flags"=dword:20000         ; POWER_STATE_OFF

; Resources available at time of device.exe initialization.
; Key names are descriptive; resource ids are definitive.
; System ids are defined in ddk\inc\resmgr.h.

;-- Resource Manager Configuration ---------------------------------------------
; HIVE BOOT SECTION
[HKEY_LOCAL_MACHINE\Drivers\Resources\IRQ]
    "Identifier"=dword:1
    "Minimum"=dword:1
    "Space"=dword:F
    "Ranges"="1,3-7,9-0xF"
    "Shared"="1,3-7,9-0xF"

[HKEY_LOCAL_MACHINE\Drivers\Resources\IO]
    "Identifier"=dword:2
    "Minimum"=dword:0
    "Space"=dword:10000
    "Ranges"="0-0x5F,0x65-0x277,0x284-0x38F,0x392-0x3DF,0x3E2-0xFFFF"
IF BSP_SERIAL1
    "Ranges"="0-0x5F,0x65-0x277,0x284-0x2F7,0x300-0x38F,0x392-0x3DF,0x3E2-0xFFFF"
ENDIF BSP_SERIAL1
IF BSP_SERIAL2
    "Ranges"="0-0x5F,0x65-0x277,0x284-0x2F7,0x300-0x38F,0x392-0x3DF,0x3E2-0x3E7,0x3F0-0xFFFF"
ENDIF BSP_SERIAL2
IF BSP_SERIAL3
    "Ranges"="0-0x5F,0x65-0x277,0x284-0x2E7,0x2F0-0x2F7,0x300-0x38F,0x392-0x3DF,0x3E2-0x3E7,0x3F0-0xFFFF"
ENDIF BSP_SERIAL3


; END HIVE BOOT SECTION
