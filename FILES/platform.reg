[HKEY_LOCAL_MACHINE\SYSTEM\GWE\UserInput]
;indicate whether mouse input automatically becomes touch input for the gesture engine
"PromoteMouseInputToTouchInput"=dword:1
;allow the touch type reported by the touch driver to be overriden. This only
;occurs if the touch driver reports TOUCHTYPE_NOTOUCH (0). Can be TOUCHTYPE_NOTOUCH (0)
;TOUCHTYPE_SINGLETOUCH (1), TOUCHTYPE_SYMMETRICTOUCH (2), or TOUCHTYPE_MULTITOUCH (3)
;By default on CEPC tchproxy is loaded, but no touch driver is loaded, so
;TOUCHTYPE_NOTOUCH is reported, so the override is used. This value is only queried
;once.
"OverrideDriverTouchType"=dword:1

; how long touch proxy will wait for touch driver to load
[HKEY_LOCAL_MACHINE\SYSTEM\GWE\TouchProxy]
    "DriverLoadTimeoutMs"=dword:1388 ; 5 seconds

